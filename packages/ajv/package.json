{
  "name": "@m9ch/vhooks-ajv",
  "version": "0.1.0-beta.10",
  "description": "Utilities for Ajv",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "prepublishOnly": "rimraf ./dist && pnpm build"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@m9ch/vhooks": "workspace:^",
    "@m9ch/vhooks-types": "workspace:^",
    "@m9ch/vhooks-utils": "workspace:^",
    "ajv": "^8.11.2",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "vue-demi": "^0.13.6"
  },
  "devDependencies": {
    "@swc/core": "^1.3.21",
    "@m9ch/vhooks-test-utils": "workspace:^",
    "jsdom": "^20.0.0",
    "rimraf": "^3.0.2",
    "vite": "^3.0.0",
    "vitest": "^0.19.1",
    "vitest-localstorage-mock": "^0.0.1",
    "vue": "^3.2.36"
  },
  "tsup": {
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "clean": true,
    "target": "es5"
  },
  "publishConfig": {
    "access": "public"
  }
}
